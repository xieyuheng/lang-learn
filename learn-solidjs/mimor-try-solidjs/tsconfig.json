{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "node16",
    "moduleResolution": "node16",
    "verbatimModuleSyntax": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client"],
    "noEmit": true,
    "isolatedModules": true,
  },
}
